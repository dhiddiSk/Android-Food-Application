<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.youcare">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/yourchoice"
        android:label="Your Choice"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".UserLocation"></activity>
        <activity android:name=".displayProducts" /> <!-- <activity android:name=".list_view_items" /> -->
        <activity android:name=".searchproducts" />
        <activity
            android:name=".UserValues"
            android:label="User values" />
        <activity
            android:name=".EatingPreferences"
            android:label="Preferences" />
        <activity
            android:name=".UserRegistration"
            android:label="New registration" />
        <activity
            android:name=".AppInfo"
            android:label="App Info" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".UserLogin"
            android:label="User login" />
    </application>

</manifest>